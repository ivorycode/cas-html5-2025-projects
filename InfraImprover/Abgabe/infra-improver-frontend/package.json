{
  "name": "infra-improver",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "buildProd": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "format": "npx prettier . --check",
    "format:fix": "npx prettier . --write",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "check": "npm run format &&  npm run lint &&  npm run build",
    "check:fix": " npm run format:fix && npm run lint:fix && npm run build",
    "clean": "rmdir /s /q node_modules && rmdir /s /q dist && rmdir /s /q .angular && del /f /q package-lock.json"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@maplibre/ngx-maplibre-gl": "^20.0.0",
    "@primeuix/themes": "^1.2.3",
    "@supabase/supabase-js": "^2.50.5",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/geojson": "^7946.0.16",
    "@types/maplibre-gl": "^1.13.2",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "chartjs-adapter-moment": "^1.0.1",
    "exif-js": "^2.3.0",
    "exifreader": "^4.31.2",
    "maplibre-gl": "^5.6.2",
    "moment": "^2.30.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.1",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.12",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@eslint/js": "^9.31.0",
    "@types/chart.js": "^2.9.41",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.1.1",
    "eslint": "^9.29.0",
    "globals": "^16.3.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.34.1"
  }
